{
  "name": "commerce-based-app",
  "version": "1.0.0",
  "description": "Commerce mock application",
  "main": "server.js",
  "engines": {
    "node": "12.13.1",
    "npm": "6.12.1"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrianromero13/commerce-template.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adrianromero13/commerce-template/issues"
  },
  "homepage": "https://github.com/adrianromero13/commerce-template#readme",
  "dependencies": {
    "aws-sdk": "^2.711.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.7-alpha.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.21",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@types/node": "^14.0.22",
    "concurrently": "^5.2.0",
    "dispatch": "^1.0.2",
    "nodemon": "^2.0.4"
  }
}
